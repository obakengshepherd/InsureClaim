{
  "ConnectionStrings": {
    "DefaultConnection": "Server=${{DB_HOST}};Database=${{DB_NAME}};User Id=${{DB_USER}};Password=${{DB_PASS}};TrustServerCertificate=True"
  },
  "JwtSettings": {
    "SecretKey": "${{JWT_SECRET}}",
    "Issuer": "InsureClaimAPI",
    "Audience": "InsureClaimFrontend",
    "ExpirationMinutes": 120
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [{ "Name": "Console" }]
  }
}
